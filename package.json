    "clear": "rimraf ./dist",
    "clear-module": "rimraf ./node_modules && rimraf package-lock.json && rimraf yarn.lock",
    "lint-js": "./node_modules/.bin/eslint *.js",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "*.js": "lint-js",
    "*.vue": "lint-js"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
