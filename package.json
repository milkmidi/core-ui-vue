{
  "name": "medialand-vue-template",
  "version": "2.1.0",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development ./node_modules/.bin/webpack-dev-server --hot ",
    "build": "cross-env NODE_ENV=production ./node_modules/.bin/webpack -p --progress --hide-modules",
    "generator": "plop --plopfile ./internal/script/generator/",
